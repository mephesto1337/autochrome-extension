[
  {
    "id": 1,
    "action": {
      "type": "modifyHeaders",
      "requestHeaders": [
        {
          "header": "X-Autochrome-Tag",
          "operation": "set",
          "value": "${COLOR}"
        }
      ]
    },
    "condition": {
      "urlFilter": "*"
    }
  },
  {
    "id": 2,
    "action": {
      "type": "modifyHeaders",
      "requestHeaders": [
        {
          "header": "User-Agent",
          "operation": "set",
          "value": "Mozilla/5.0 (X11; Linux x86_64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/106.0.0.0 Safari/537.36 autochrome/${COLOR}"
        }
      ]
    },
    "condition": {
      "urlFilter": "*"
    }
  }
]
